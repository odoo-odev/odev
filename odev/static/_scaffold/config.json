{
  "default": {
    "assets": {
      "file_name_by_fields": [
        "asset_template"
      ],
      "folder_name": "views",
      "lang": "xml",
      "template": "scaffold_assets.jinja"
    },
    "controllers": {
      "file_name": "controllers",
      "folder_name": "controllers",
      "lang": "py",
      "template": "scaffold_controller.jinja"
    },
    "edi": {
      "file_name_by_fields": [
        "model"
      ],
      "folder_name": "data",
      "lang": "xml",
      "template": "scaffold_edi_integration.jinja"
    },
    "end_migrate": {
      "file_name": "end-10",
      "folder_name": "migrations/{{version}}",
      "lang": "py",
      "template": "scaffold_post-10.jinja"
    },
    "init": {
      "file_name": "__init__",
      "lang": "py",
      "template": "scaffold_init.jinja"
    },
    "js": {
      "file_name_by_fields": [
        "file_name"
      ],
      "folder_name": "static/src/js",
      "lang": "js",
      "template": "scaffold_default_js.jinja"
    },
    "manifest": {
      "file_name": "__manifest__",
      "lang": "py",
      "template": "scaffold_manifest.jinja"
    },
    "models": {
      "base.action.rule": {
        "file_name_by_fields": [
          "model_id.0.xml_name"
        ],
        "folder_name": "data/action_rules",
        "includes": [
          {
            "comodel_name": "ir.model",
            "inverse_name": "id",
            "key": "model_id"
          },
          {
            "comodel_name": "ir.actions.server",
            "inverse_name": "id",
            "key": "server_action_ids"
          }
        ],
        "lang": "xml",
        "split": true,
        "template": "base_action_rule.jinja"
      },
      "base.automation": {
        "file_name_by_fields": [
          "model_id.0.xml_name"
        ],
        "folder_name": "data",
        "includes": [
          {
            "comodel_name": "ir.model",
            "inverse_name": "id",
            "key": "model_id"
          }
        ],
        "lang": "xml",
        "split": true,
        "template": "base_automation.jinja"
      },
      "default": {
        "_comment": "Export all other models as data",
        "file_name_by_fields": [
          "__xml_model"
        ],
        "folder_name": "data",
        "includes": [
          {
            "comodel_name": "ir.model",
            "inverse_name": "id",
            "key": "model_id"
          }
        ],
        "lang": "xml",
        "split": true,
        "template": "data.jinja"
      },
      "ir.actions.act_url": {
        "file_name_by_fields": [
          "__xml_model"
        ],
        "folder_name": "data",
        "lang": "xml",
        "template": "ir_actions_act_url.jinja"
      },
      "ir.actions.act_window": {
        "file_name_by_fields": [
          "res_model",
          "xml_id"
        ],
        "folder_name": "data",
        "lang": "xml",
        "split": true,
        "template": "ir_actions_act_window.jinja"
      },
      "ir.actions.act_window.view": {
        "file_name_by_fields": [
          "res_model",
          "xml_id"
        ],
        "folder_name": "views",
        "include_relations": [
          "view_id",
          "act_window_id"
        ],
        "lang": "xml",
        "split": true,
        "template": "ir_actions_act_window_view.jinja"
      },
      "ir.actions.report": {
        "file_name_by_fields": [
          "model"
        ],
        "file_name_suffix": "_report",
        "folder_name": "reports",
        "include_relations": [
          "paperformat_id",
          "binding_model_id"
        ],
        "lang": "xml",
        "split": true,
        "template": "ir_actions_report.jinja"
      },
      "ir.actions.report.xml": {
        "file_name_by_fields": [
          "model"
        ],
        "folder_name": "views",
        "lang": "xml",
        "split": true,
        "template": "ir_actions_report.jinja"
      },
      "ir.actions.server": {
        "file_name_by_fields": [
          "model_name"
        ],
        "folder_name": "data/server_actions",
        "include_relations": [
          "model_id",
          "binding_model_id",
          "group_id"
        ],
        "lang": "xml",
        "parent": "ir.model",
        "split": true,
        "template": "ir_actions_server.jinja"
      },
      "ir.cron": {
        "file_name_by_fields": [
          "res_model",
          "__xml_id"
        ],
        "file_name_suffix": "_cron",
        "folder_name": "data",
        "include_relations": [
          "model_id"
        ],
        "lang": "xml",
        "split": true,
        "template": "ir_cron.jinja"
      },
      "ir.model": {
        "file_name_by_fields": [
          "model"
        ],
        "folder_name": "models",
        "include_info": false,
        "lang": {
          "saas": "xml",
          "sh": "py"
        },
        "split": true,
        "template": "ir_model.jinja"
      },
      "ir.model.access": {
        "file_name": "ir.model.access",
        "folder_name": "security",
        "include_relations": [
          "group_id",
          "model_id"
        ],
        "lang": "csv",
        "template": "ir_model_access.jinja"
      },
      "ir.rule": {
        "file_name_by_fields": [
          "model_name"
        ],
        "file_name_suffix": "_security",
        "folder_name": "security",
        "include_relations": [
          "model_id"
        ],
        "lang": "xml",
        "template": "ir_rule.jinja"
      },
      "ir.ui.menu": {
        "file_name_by_fields": [
          "__xml_module"
        ],
        "file_name_suffix": "_menus",
        "folder_name": "views",
        "include_relations": [
          "parent_id",
          "action"
        ],
        "lang": "xml",
        "template": "ir_ui_menu.jinja"
      },
      "ir.ui.view": {
        "callback": [
          "_get_odoo_xml"
        ],
        "file_name_by_fields": [
          "model",
          "__xml_id"
        ],
        "file_name_suffix": "_views",
        "folder_name": "views",
        "include_relations": [
          "inherit_id"
        ],
        "lang": "xml",
        "split": true,
        "template": "ir_ui_view.jinja"
      },
      "res.groups": {
        "file_name_by_fields": [
          "__xml_module"
        ],
        "file_name_suffix": "_groups",
        "folder_name": "security",
        "include_relations": [
          "category_id",
          "implied_ids"
        ],
        "lang": "xml",
        "template": "res_groups.jinja"
      }
    },
    "post_migrate": {
      "file_name": "post-10",
      "folder_name": "migrations/{{version}}",
      "lang": "py",
      "template": "scaffold_post-10.jinja"
    },
    "pre_migrate": {
      "file_name": "pre-10",
      "folder_name": "migrations/{{version}}",
      "lang": "py",
      "template": "scaffold_pre-10.jinja"
    },
    "project_files": {
      "files": [
        ".gitignore"
      ]
    },
    "readme": {
      "file_name": "README",
      "lang": "md",
      "template": "scaffold_readme.jinja"
    },
    "regexp": {
      "*": [
        {
          "fields": [
            "model",
            "model_id",
            "xml_id",
            "xml_name",
            "model_name",
            "name",
            "res_model",
            "code"
          ],
          "regexp": [
            {
              "pattern": "account([\\._])invoice([\\._])line",
              "replace": "account\\1move\\2line"
            },
            {
              "pattern": "account([\\._])invoice",
              "replace": "account\\1move"
            }
          ]
        },
        {
          "fields": [
            "name"
          ],
          "regexp": [
            {
              "pattern": "Invoice Line",
              "replace": "Move Line"
            }
          ]
        }
      ],
      "ir.model.fields": [],
      "ir.ui.view": [
        {
          "fields": [
            "arch"
          ],
          "regexp": [
            {
              "pattern": "report.html_container",
              "replace": "web.html_container"
            }
          ]
        }
      ]
    },
    "reports": {
      "file_name_by_fields": [
        "model",
        "view_id"
      ],
      "folder_name": "report",
      "lang": "xml",
      "template": "scaffold_report.jinja"
    },
    "requirements": {
      "file_name": "requirements",
      "folder_name": "..",
      "lang": "txt",
      "template": "scaffold_requirements.jinja"
    },
    "scss": {
      "file_name_by_fields": [
        "file_name"
      ],
      "folder_name": "static/src/scss",
      "lang": "scss",
      "template": "scaffold_default_scss.jinja"
    },
    "sql": {
      "file_name_by_fields": [
        "model"
      ],
      "folder_name": "security",
      "lang": "sql",
      "split": true,
      "template": "scaffold_sql.jinja"
    },
    "unit_test": {
      "file_name": "test_{{model}}",
      "folder_name": "tests",
      "lang": "py",
      "template": "scaffold_unit_test.jinja"
    },
    "unit_test_common": {
      "file_name": "common",
      "folder_name": "tests",
      "lang": "py",
      "template": "scaffold_unit_test_common.jinja"
    }
  }
}
